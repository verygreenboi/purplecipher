{
  "version": 2,
  "projects": {
    "acrifa-auth": "libs/acrifa-auth",
    "acrifa-landing": "libs/acrifa-landing",
    "acrifa-landing-app": {
      "projectType": "application",
      "root": "apps/acrifa-landing-app",
      "sourceRoot": "apps/acrifa-landing-app/src",
      "prefix": "purplecipher",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/acrifa-landing-app/browser",
            "index": "apps/acrifa-landing-app/src/index.html",
            "main": "apps/acrifa-landing-app/src/main.ts",
            "polyfills": "apps/acrifa-landing-app/src/polyfills.ts",
            "tsConfig": "apps/acrifa-landing-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/acrifa-landing-app/src/favicon.ico",
              "apps/acrifa-landing-app/src/assets",
              {
                "input": "libs/shared/assets/src/assets",
                "glob": "**/*",
                "output": "assets"
              }
            ],
            "styles": ["apps/acrifa-landing-app/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/acrifa-landing-app/src/environments/environment.ts",
                  "with": "apps/acrifa-landing-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "acrifa-landing-app:build:production"
            },
            "development": {
              "browserTarget": "acrifa-landing-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "acrifa-landing-app:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/acrifa-landing-app/src/**/*.ts",
              "apps/acrifa-landing-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/acrifa-landing-app"],
          "options": {
            "jestConfig": "apps/acrifa-landing-app/jest.config.js",
            "passWithNoTests": true
          }
        },
        "server": {
          "executor": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/apps/acrifa-landing-app/server",
            "main": "apps/acrifa-landing-app/server.ts",
            "tsConfig": "apps/acrifa-landing-app/tsconfig.server.json",
            "inlineStyleLanguage": "scss"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "apps/acrifa-landing-app/src/environments/environment.ts",
                  "with": "apps/acrifa-landing-app/src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "executor": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "acrifa-landing-app:build:development",
              "serverTarget": "acrifa-landing-app:server:development"
            },
            "production": {
              "browserTarget": "acrifa-landing-app:build:production",
              "serverTarget": "acrifa-landing-app:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "executor": "@nguniversal/builders:prerender",
          "options": {
            "routes": ["/"]
          },
          "configurations": {
            "production": {
              "browserTarget": "acrifa-landing-app:build:production",
              "serverTarget": "acrifa-landing-app:server:production"
            },
            "development": {
              "browserTarget": "acrifa-landing-app:build:development",
              "serverTarget": "acrifa-landing-app:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "acrifa-landing-app-e2e": {
      "root": "apps/acrifa-landing-app-e2e",
      "sourceRoot": "apps/acrifa-landing-app-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/acrifa-landing-app-e2e/cypress.json",
            "devServerTarget": "acrifa-landing-app:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "acrifa-landing-app:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/acrifa-landing-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "layout-helpers": {
      "projectType": "library",
      "root": "libs/layout-helpers",
      "sourceRoot": "libs/layout-helpers/src",
      "prefix": "purplecipher",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/layout-helpers"],
          "options": {
            "project": "libs/layout-helpers/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/layout-helpers/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/layout-helpers/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/layout-helpers"],
          "options": {
            "jestConfig": "libs/layout-helpers/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/layout-helpers/src/**/*.ts",
              "libs/layout-helpers/src/**/*.html"
            ]
          }
        }
      }
    },
    "layout-navbar": "libs/layout/navbar",
    "shared-assets": "libs/shared/assets",
    "shared-base-class": "libs/shared/base-class",
    "shared-model": "libs/shared/model",
    "util": {
      "projectType": "library",
      "root": "libs/util",
      "sourceRoot": "libs/util/src",
      "prefix": "purplecipher",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/util"],
          "options": {
            "project": "libs/util/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/util/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/util/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/util"],
          "options": {
            "jestConfig": "libs/util/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/util/src/**/*.ts",
              "libs/util/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "acrifa-landing-app"
}
